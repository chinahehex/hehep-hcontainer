{
	"name": "hehex/hehep-hcontainer",
	"description": "hehep-hcontainer 是一个di容器基础组件,提供类的实例化,对象属性依赖注入,AOP 拦截,容器文件扫描,等等功能",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "hehe"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"autoload": {
		"psr-4": {
			"hehe\\core\\hcontainer\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"hcontainer\\test\\": "tests/"
		}
	},
	"scripts": {
		"test:win": ["@test:php7","@test:php8"],
		"test:php7": "D:/bin/phpstudy_pro/Extensions/php/php7.4.3nts/php.exe ../vendor/bin/phpunit",
		"test:php8": "D:/bin/phpstudy_pro/Extensions/php/php8.2.9nts/php.exe ../vendor/bin/phpunit"
	}
}
